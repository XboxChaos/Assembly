<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="ExtryzeAccentBrush" x:Name="ExtryzeAccentBrush" Color="#CA5100"/>
    <SolidColorBrush x:Key="ExtryzeAccentBrushSecondary" x:Name="ExtryzeAccentBrushSecondary" Color="#D66F2B"/>
    
    <Color x:Key="ExtryzeAccentBrushColour" A="#FF" R="#CA" G="#51" B="#00" />
    <Color x:Key="ExtryzeAccentBrushColourSecondary"  A="#FF" R="#D9" G="#6F" B="#2B" />
    
    <BitmapImage x:Key="ExtryzeAccentLogo" x:Name="ExtryzeAccentLogo" UriSource="/Assembly;component/Metro/Images/orange_logo_small.png" />
    
</ResourceDictionary>