<UserControl x:Class="Assembly.Metro.Controls.AvalonDockMetro.Vectors.VTabGroup"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Width="14" Height="13">
	<Grid>
		<StackPanel Orientation="Horizontal">
            <Rectangle Width="7" Fill="{DynamicResource ExtryzeAccentBrush}" />
            <Rectangle Width="7" Fill="{DynamicResource ExtryzeAccentBrush}" />           
		</StackPanel>

		<Rectangle Fill="#232324" HorizontalAlignment="Left" Width="5" Margin="1,3,1,1"/>
		<Rectangle Fill="#232324" HorizontalAlignment="Right" Width="5" Margin="1,3,1,1"/>
	</Grid>
</UserControl>