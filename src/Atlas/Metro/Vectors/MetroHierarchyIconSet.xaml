<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<ControlTemplate x:Key="HierarchyClosedFolder">
		<Canvas Margin="8,2,5,0" Width="15" Height="16">
			<Path Fill="#252526"
				  SnapsToDevicePixels="True"
				  Data="F1 M 0,2 L 0,13 15,13 15,0 7,0 5,2 Z" />
			
			<Path Fill="#d7ab69"
				  SnapsToDevicePixels="True"
				  Data="F1 M 1,3 L 1,12 14,12 14,1 7,1 5,3 6,3 7,2 13,2 13,3 1,3 Z" />
		</Canvas>
	</ControlTemplate>

	<ControlTemplate x:Key="HierarchyGenericTag">
		<Canvas Margin="8,6,4,0" Width="16" Height="16">
			<Path Fill="#252526"
				  SnapsToDevicePixels="True"
				  Data="F1 M 0,1 L 6,1 6,3 8,3 8,0 16,0 16,9 8,9 8,6 6,6 6,8 0,8 Z" />

			<Path Fill="#c4c4c4"
				  SnapsToDevicePixels="True"
				  Data="F1 M 1,2 L 5,2 5,7 1,7 1,2 Z
						M 6,4 L 8,4 8,5 6,5 6,4 Z
						M 9,1 L 15,1 15,8 9,8 9,1 Z" />
		</Canvas>
	</ControlTemplate>
	<ControlTemplate x:Key="HierarchySnd!Tag">
		<Canvas Margin="8,0,4,0" Width="16" Height="16">
			<Path Fill="#252526"
				  SnapsToDevicePixels="True"
				  Data="F1 M 0,4 L 3,4 7,0 10,0 10,4 11,4 11,11 10,11 10,15 7,15 3,11 0,11 0,4 Z
						M 11,4 L 11,3 12,3 12,2 14,2 14,3 15,3 15,4 16,4 16,11 15,11 15,12 14,12 14,13 12,13 12,12 11,12 11,11 Z" />

			<Path Fill="#c4c4c4"
				  SnapsToDevicePixels="True"
				  Data="F1 M 1,5 L 4,5 8,1 9,1 9,6 11,6 11,9 9,9 9,14 8,14 4,10 1,10 1,5 Z
						M 12,6 L 12,5 13,5 13,4 14,4 14,5 13,5 13,6 12,6 Z
						M 13,7 L 15,7 15,8 13,8 13,7 Z
						M 12,9 L 13,9 13,10 14,10 14,11 13,11 12,10 12,9 Z" />
		</Canvas>
	</ControlTemplate>
	<ControlTemplate x:Key="HierarchyBitmTag">
		<Canvas Margin="8,-1,4,0" Width="16" Height="16">
			<Path Fill="#252526"
				  SnapsToDevicePixels="True"
				  Data="F1 M 0,0 L 16,0 16,16 0,16 Z" />

			<Path Fill="#c4c4c4"
				  SnapsToDevicePixels="True"
				  Data="F1 M 1,1 L 15,1 15,15 1,15 1,2 2,2 2,14 14,14 14,2 1,2 1,1 Z
						M 2,9 L 4,6 6,6 8,11 10,11 12,10 14,10 15,11 15,12 14,12 13,11 11,11 10,12 8,12 6,9 4,9 2,12 Z
						M 8,4 L 9,4 9,3 10,3 10,4 11,4 11,5 10,5 11,6 10,6 10,5 9,5 8,4 Z" />
		</Canvas>
	</ControlTemplate>
	<ControlTemplate x:Key="HierarchyModeTag">
		<Canvas Margin="8,-1,4,0" Width="16" Height="16">
			<Path Fill="#252526"
				  SnapsToDevicePixels="True"
				  Data="F1 M 7,0 L 14,3 14,12 7,16 0,12 0,4 Z" />

			<Path Fill="#c4c4c4"
				  SnapsToDevicePixels="True"
				  Data="F1 M 7,1 L 13,4 7,7 1,4 Z" />

			<Path Fill="#AEAEAE"
				  SnapsToDevicePixels="True"
				  Data="F1 M 7,7 L 13,4 13,12 7,15 Z" />
			
			<Path Fill="#8C8C8C"
				  SnapsToDevicePixels="True"
				  Data="F1 M 1,4 L 7,7 7,15 1,12 Z" />
		</Canvas>
	</ControlTemplate>

	<ControlTemplate x:Key="HierarchySpecializedModeTag">
		<Canvas Margin="0,0,4,0" Width="14" Height="16">
			<Path Fill="#252526" SnapsToDevicePixels="True"
				  Data="M 2,1 L 10,1 14,5 14,15 10,15 10,16 2,16 0,14 0,12 2,11 Z"/>
			
			<Path Fill="#c4c4c4"
				  SnapsToDevicePixels="True"
				  Data="M 2,11 L 3,11 3,12 2,12 2,14 3,14 3,15 2,15 2,14 1,14 1,12 2,12 Z
						M 9,11 L 10,11 10,12 11,12 11,14 10,14 10,15 9,15 9,14 10,14 10,12 9,12 Z
						M 3,2 L 10,2 13,5 13,14 12,14 12,6 9,6 9,3 4,3 4,10 3,10 z" />

			<Path Fill="#40c4ff"
				  SnapsToDevicePixels="True"
				  Data="M 4,11 L 8,11 8,15 4,15 Z" />
		</Canvas>
	</ControlTemplate>
	<ControlTemplate x:Key="HierarchySpecializedSnd!Tag">
		<Canvas Margin="0,0,4,0" Width="14" Height="16">
			<Path Fill="#252526" SnapsToDevicePixels="True"
				  Data="M 2,1 L 10,1 14,5 14,15 7,15 7,16 3,16 2,14 2,10 2,10 Z"/>

			<Path Fill="#40c4ff" SnapsToDevicePixels="True"
				  Data="M 3,12 L 4,12 6,10 6,12 7,12 7,13 6,13 6,15 4,13 3,13 Z"/>

			<Path Fill="#c4c4c4"
				  SnapsToDevicePixels="True"
				  Data="M 3,2 L 10,2 13,5 13,14 12,14 12,6 9,6 9,3 4,3 4,10 3,10 z" />
		</Canvas>
	</ControlTemplate>

</ResourceDictionary>