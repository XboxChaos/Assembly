<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Atlas.Metro.Vectors.MinimizeModule"
	x:Name="UserControl"
	d:DesignWidth="640" d:DesignHeight="480" Width="34" Height="26">

	<Grid x:Name="LayoutRoot">
		<Grid Width="10" Height="10">
			<Rectangle Fill="#FFF4F4F5" Margin="0,0,1,0" Width="9" Height="3" VerticalAlignment="Bottom"
			           d:LayoutOverrides="Height" />
		</Grid>
		<Rectangle Fill="White" Opacity="0" />
	</Grid>
</UserControl>