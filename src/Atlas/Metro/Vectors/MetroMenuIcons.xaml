<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<ControlTemplate x:Key="MenuVectorIconSave">
		<Canvas Margin="5,-7,0,0">
			<Path Fill="#6ebbff"
				  SnapsToDevicePixels="True"
				  Data="F1 M 0,0 L 0,12 12,12 12,2 10,0 9,0 9,5 3,5 3,0 0,0 M 6,0 L 6,3 8,3 8,0 6,0 Z" />
		</Canvas>
	</ControlTemplate>

	<ControlTemplate x:Key="MenuVectorAdvancedMemoryMod">
		<Canvas Margin="-10,-10,0,0">
			<Path Fill="#2d2d30" SnapsToDevicePixels="True"
				  Data="M 7,2 L 9,2 10,3 11,2 13,2 14,3 14,6 15,6 15,9 14,9 14,13 12,13 12,18 8,18 8,13 6,13 6,9 5,9 5,6 6,6 6,3 Z" />
			
			<Path Fill="#c7c7c7" SnapsToDevicePixels="True"
				  Data="M 7,3 L 9,3 9,6 7,6 Z
						M 11,3 L 13,3 13,6 11,6 Z
						M 9,13 L 11,13 11,17 9,17 Z
						M 6,7 L 14,7 14,8 13,8 13,12 7,12 7,8 6,8 Z" />
		</Canvas>
	</ControlTemplate>

	<ControlTemplate x:Key="MenuVectorNetworkSession">
		<Canvas Margin="-10,-10,0,0">
			<Ellipse Margin="3,4,0,0" Width="6" Height="6" Stroke="#c7c7c7" StrokeThickness="2" />
			<Ellipse Margin="11,12,0,0" Width="6" Height="6" Stroke="#c7c7c7" StrokeThickness="2" />

			<Path Fill="#c7c7c7" SnapsToDevicePixels="True"
				  Data="M 8,6 L 15,6 15,13 13,13 13,8 8,8 Z" />
		</Canvas>
	</ControlTemplate>

	<ControlTemplate x:Key="MenuVectorXbox360FileBrowser">
		<Canvas Margin="-10,-10,0,0">
			<Path Fill="#2d2d30"
				  SnapsToDevicePixels="True"
				  Data="F1 M 6,3 L 18,3 18,16 3,16 3,13 2,11 2,9 3,9 3,5 6,5 Z" />
			
			<Path Fill="#77bb44"
				  SnapsToDevicePixels="True"
				  Data="M 8,4 L 17,4 17,15 5,15 4,13 4,13 3,11 3,10 4,10 4,6 7,6 Z" />

			<Path Fill="#2d2d30"
				  SnapsToDevicePixels="True"
				  Data="F1 M 5,7 L 8,7 9,5 16,5 16,14 15,14 13,10 5,10 Z" />
		</Canvas>
	</ControlTemplate>

</ResourceDictionary>